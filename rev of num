import java.util.*;
class Main{
    public static void reverse_number(int n)
    {
        int rem;
        int rev=0;
        while(n!=0)
        {
            rem=n%10;
            rev=(rev*10)+rem;
            n=n/10;
        }
        System.out.print(rev);
    }
    public static void main (String[] args) {
        
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        reverse_number(n);
    }
}
