import java.util.*;
class Main{
    public static void main (String[] args) {
        Scanner sc=new Scanner (System.in);
        int n=sc.nextInt();
        
        int sum=0;
        while(n>0)
        {
            int rem=n%10;
            int fact=1;
            while(rem>0)
            {
                fact=fact*rem;              //Strong number 145 1!+4!+5!
                rem--;
            }
        sum=fact+sum;
        n=n/10;
        }
        System.out.println(sum);
    }
}
